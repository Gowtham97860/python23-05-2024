def find_repetitive_element(arr):
    n = len(arr)
    if n < 2:
        print("Array elements are not repeated")
        return
    for i in range(1, n):
        if arr[i] == arr[i-1]:
            print("The repetitive element =", arr[i])
            return
    print("Array elements are not repeated")

# Example usage
array = [1, 23, 1, 4, 5]
find_repetitive_element(array)
